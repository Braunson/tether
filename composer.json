{
    "name": "interfaceable/tether",
    "description": "Abstract Inertia.js pages and props into re-usable PHP classes for the Laravel adapter.",
    "keywords": ["laravel", "inertia"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Interfaceable\\Tether\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Percy Mamedy",
            "email": "percymamedy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "inertiajs/inertia-laravel": "^0.6.3"
    },
    "scripts": {
        "lint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Interfaceable\\TetherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.2"
    }
}